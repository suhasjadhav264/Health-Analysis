   "source": [
    "df['height'] = df['height']/100\n",
    "df['BMI'] = df['weight']/df['height']**2\n",
    "ok = []\n",
    "\n",
    "for w in df['BMI']:\n",
    "    if w > 25:\n",
    "        ok.append(1)\n",
    "    else:\n",
    "\n",
    "        ok.append(0)\n",
    "df['Overweight'] = ok\n",
    "df"
